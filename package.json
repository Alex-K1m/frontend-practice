{
  "name": "html-templates",
  "private": true,
  "description": "A set of html templates made while practicing core web coding skills.",
  "version": "0.1.0",
  "author": "Alex Kim",
  "scripts": {
    "clean": "gulp clean",
    "start": "gulp",
    "lint": "run-p -c lint:*",
    "lint:js": "eslint .",
    "lint:css": "stylelint '**/*.scss'",
    "lint:html": "prettier -c '**/*.pug'",
    "fix": "run-p -c fix:*",
    "fix:js": "eslint --fix .",
    "fix:css": "stylelint --fix '**/*.scss'",
    "fix:html": "prettier --write '**/*.pug'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.scss": "stylelint",
    "*.pug": "prettier -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-K1m/html-templates.git"
  },
  "bugs": {
    "url": "https://github.com/Alex-K1m/html-templates/issues"
  },
  "homepage": "https://alex-k1m.github.io/html-templates/",
  "devDependencies": {
    "@prettier/plugin-pug": "^1.3.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.1",
    "delete": "^1.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {}
}
