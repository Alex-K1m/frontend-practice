{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend projects for practicing with various tech",
  "license": "MIT",
  "repository": "git@github.com:Alex-K1m/frontend-practice.git",
  "author": "Alex Kim <45559664+Alex-K1m@users.noreply.github.com>",
  "homepage": "https://alex-k1m.github.io/frontend-practice/",
  "bugs": "https://github.com/Alex-K1m/frontend-practice/issues",
  "scripts": {
    "boot": "lerna bootstrap",
    "build": "lerna run build --parallel --since $(git describe --abbrev=0 --first-parent HEAD^",
    "build:all": "lerna run build --parallel",
    "bump": "lerna version",
    "clean:cache": "lerna run --parallel clean:cache",
    "fix:code": "eslint --fix .",
    "fix:misc": "prettier --write \"**/*.{pug,json,yml,yaml,md}\"",
    "fix:styles": "stylelint --fix \"**/*.scss\"",
    "lint:code": "eslint .",
    "lint:misc": "prettier --check \"**/*.{pug,json,yml,yaml,md}\"",
    "lint:styles": "stylelint \"**/*.scss\"",
    "list": "lerna ls -la",
    "status": "lerna changed -a",
    "test": "lerna run test",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^1.8.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "lerna": "^3.22.1",
    "prettier": "^2.1.2",
    "prettier-plugin-package": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-sass-guidelines": "^7.1.0"
  },
  "workspaces": [
    "projects/*",
    "packages/*"
  ]
}
